{
  "name": "riverpod",
  "displayName": "riverpod",
  "description": "A riverpod extension",
  "publisher": "HoussemBousmaha",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
